{
    "ContainerSettings" : [
      {
          "Image" : "nginx",
          "Tag" : "latest",
          "Ports" : [
              "80/tcp:80"
          ],
          "Volumes" : [
              "/docker/nginx:/usr/share/nginx/html:ro"
          ]
      },
      {
        "Image": "mongo",
        "Tag": "latest",
        "Ports": [
          "27017/tcp:27017",
          "28017/tcp:28017"
        ],
        "Volumes": [
          "c:/docker/mongodb:/data/db:rw"
        ],
        "Networks": [
          "mongonet"
        ],
        "$$$$Envs": [
          "MONGO_INITDB_ROOT_USERNAME=root",
          "MONGO_INITDB_ROOT_PASSWORD=rootpass"
        ]
      },
      {
        "Image": "hello-mongo",
        "Name": "asdf",
        "Tag": "0.14",
        "Networks": [
          "mongonet"
        ]
      }
    ]
}